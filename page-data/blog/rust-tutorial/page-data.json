":{}}],"data":{}}],"data":{}},{"nodeType":"list-item","content":[{"nodeType":"paragraph","content":[{"nodeType":"text","value":"Minimal rust kernel (","marks":[{"type":"bold"}],"data":{}},{"nodeType":"text","value":" creating a minimal 64-bit Rust kernel for the x84 arhitecutre)","marks":[],"data":{}}],"data":{}}],"data":{}},{"nodeType":"list-item","content":[{"nodeType":"paragraph","content":[{"nodeType":"text","value":" VGA Text Mode  (","marks":[{"type":"bold"}],"data":{}},{"nodeType":"text","value":" Creating a simple way to print text on the screen","marks":[],"data":{}},{"nodeType":"text","value":")","marks":[{"type":"bold"}],"data":{}}],"data":{}}],"data":{}}],"data":{}},{"nodeType":"paragraph","content":[{"nodeType":"text","value":"","marks":[{"type":"bold"}],"data":{}}],"data":{}},{"nodeType":"heading-1","content":[{"nodeType":"text","value":"Free Standing kernel","marks":[],"data":{}}],"data":{}},{"nodeType":"paragraph","content":[{"nodeType":"text","value":"First we disabled standard library (we cannot use any os dependent libraries ).Also we will need panic implementation ","marks":[],"data":{}}],"data":{}},{"nodeType":"paragraph","content":[{"nodeType":"text","value":"Panic implementation it called when ever something fucks up so we save memory and time by aborting the compile and allocation of the memory for the failed or buggy function","marks":[],"data":{}}],"data":{}},{"nodeType":"paragraph","content":[{"nodeType":"text","value":"","marks":[],"data":{}}],"data":{}},{"nodeType":"paragraph","content":[{"nodeType":"text","value":"Speaking of compiler and runtime we also need to be careful with compilation errors\nThere are 3 types of errors:","marks":[],"data":{}}],"data":{}},{"nodeType":"unordered-list","content":[{"nodeType":"list-item","content":[{"nodeType":"paragraph","content":[{"nodeType":"text","value":"compiler warnings","marks":[{"type":"bold"},{"type":"italic"}],"data":{}},{"nodeType":"text","value":";(Don't ignore but tbh it isn't something severe enough to keep your code from compiling, unless you instruct your compiler to treat them as errors )","marks":[],"data":{}}],"data":{}}],"data":{}},{"nodeType":"list-item","content":[{"nodeType":"paragraph","content":[{"nodeType":"text","value":"compiler errors","marks":[{"type":"bold"},{"type":"italic"}],"data":{}},{"nodeType":"text","value":";(on the other hand we have errors they are conditions that prevent the compiler from compilation of the file or code )","marks":[],"data":{}}],"data":{}}],"data":{}},{"nodeType":"list-item","content":[{"nodeType":"paragraph","content":[{"nodeType":"text","value":"linker errors","marks":[{"type":"italic"},{"type":"bold"}],"data":{}},{"nodeType":"text","value":";( Don;t care for syntax , care for finding definition for functions ,classes etc e.g could not find definition x)","marks":[],"data":{}}],"data":{}}],"data":{}}],"data":{}},{"nodeType":"paragraph","content":[{"nodeType":"text","value":"Why we need to think of linker errors , its because of different os like Linux , win etc... we could use ","marks":[],"data":{}},{"nodeType":"text","value":"rust bare metal target ","marks":[{"type":"bold"}],"data":{}},{"nodeType":"text","value":"it automatically tries to detect what type of executable is it \".exe\" or something else","marks":[],"data":{}}],"data":{}},{"nodeType":"paragraph","content":[{"nodeType":"text","value":"","marks":[],"data":{}},{"nodeType":"hyperlink","content":[{"nodeType":"text","value":"For more information go to my github project ","marks":[],"data":{}}],"data":{"uri":"https://github.com/bugenzi"}},{"nodeType":"text","value":"","marks":[],"data":{}}],"data":{}},{"nodeType":"paragraph","content":[{"nodeType":"text","value":"","marks":[],"data":{}}],"data":{}}]}}}},"pageContext":{"slug":"rust-tutorial"}},"staticQueryHashes":[]}